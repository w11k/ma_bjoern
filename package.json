  "license": "ISC",
  "private": true,
  "dependencies": {
    "framework7": "^3.5.2",
    "framework7-icons": "^2.0.5",
    "framework7-react": "^3.5.2",
    "material-design-icons": "^3.0.1",
    "moment": "^2.22.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-easy-params": "^2.0.2",
    "react-easy-state": "^6.0.6",
    "react-scripts": "^2.1.1"
  },
  "devDependencies": {
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "cpy-cli": "^2.0.0",
    "gh-pages": "^2.0.1"
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
