{
  "name": "react-nui",
  "version": "1.0.0",
  "description": "SPA mit React und nativem UI (Framework7)",
  "main": "index.html",
  "homepage": "https://w11k.github.io/ma_bjoern/react-nui",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "copy-fonts": "cpy node_modules/framework7-icons/fonts/*.* src/fonts && cpy node_modules/material-design-icons/iconfont/*.{eot,ttf,woff,woff2} src/fonts",
    "postinstall": "npm run copy-fonts",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build -b master -e react-nui"
  },
